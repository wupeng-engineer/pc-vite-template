{
  "name": "vite-vue-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config build/vite.dev.ts",
    "build": "yarn lint-fix && vue-tsc --noEmit && vite build --config build/vite.pro.ts",
    "serve": "vite preview",
    "lint:js": "eslint --config ./.eslintrc  ./src/**/*.{ts,tsx,vue}",
    "lint-fix:js": "eslint --config ./.eslintrc  ./src/**/*.{ts,tsx,vue} --fix",
    "lint:style": "stylelint --config ./.stylelintrc ./src/**/*.{less,vue,tsx}",
    "lint-fix:style": "stylelint --config ./.stylelintrc ./src/**/*.{less,vue,tsx} --fix",
    "lint": "npm-run-all --parallel lint:*",
    "lint-fix": "npm-run-all --parallel lint-fix:*"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.23.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue-tsc": "^0.0.15",
    "webpack-merge": "^5.7.3"
  }
}
