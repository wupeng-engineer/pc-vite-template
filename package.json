{
  "name": "pc-vite-template",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development vite --config build/vite.dev.ts",
    "pro": "vue-tsc --noEmit && vite build --config build/vite.pro.ts",
    "build": "cross-env NODE_ENV=production yarn lint-fix && yarn pro",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true yarn pro",
    "serve": "vite preview",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test}/**/*.{ts,tsx,vue}",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx}",
    "lint": "npm-run-all --parallel lint:*",
    "lint-fix:js": "eslint --config .eslintrc.js  ./src/**/*.{ts,tsx,vue} --fix",
    "lint-fix:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx} --fix",
    "lint-fix": "npm-run-all --parallel lint-fix:*",
    "test": "jest --passWithNoTests",
    "test:cover": "jest --passWithNoTests --coverage --reporters jest-stare "
  },
  "dependencies": {
    "@types/qs": "^6.9.6",
    "lodash": "^4.17.21",
    "pinia": "^2.0.0-beta.2",
    "qs": "^6.10.1",
    "ramda": "^0.27.1",
    "tailwindcss": "^2.1.4",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.0",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "jest": "^27.0.4",
    "jest-stare": "^2.2.1",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-import": "^14.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-optimize-lodash-imports": "^1.0.2",
    "rollup-plugin-visualizer": "^5.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue-test": "^1.0.0",
    "vue-tsc": "^0.1.7",
    "webpack-merge": "^5.7.3"
  },
  "jest-stare": {
    "merge": "true",
    "resultDir": "jest-stare",
    "log": "true",
    "resultHtml": "index.html",
    "resultJson": "data.json",
    "jestStareConfigJson": "jest-stare-config.json",
    "coverageLink": "../coverage/lcov-report/index.html"
  }
}
