{
  "name": "vite-vue-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --config build/vite.dev.ts",
    "pro": "vue-tsc --noEmit && vite build --config build/vite.pro.ts",
    "build": "cross-env NODE_ENV=production yarn lint-fix && yarn pro",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true yarn pro",
    "serve": "vite preview",
    "lint:js": "eslint --config .eslintrc.js  ./src/**/*.{ts,tsx,vue}",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx}",
    "lint": "npm-run-all --parallel lint:*",
    "lint-fix:js": "eslint --config .eslintrc.js  ./src/**/*.{ts,tsx,vue} --fix",
    "lint-fix:style": "stylelint --config .stylelintrc.js ./src/**/*.{less,vue,tsx} --fix",
    "lint-fix": "npm-run-all --parallel lint-fix:*"
  },
  "dependencies": {
    "ramda": "^0.27.1",
    "rxjs": "^6.6.7",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/ramda": "^0.27.40",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-visualizer": "^5.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "vite": "^2.1.5",
    "vue-tsc": "^0.0.15",
    "webpack-merge": "^5.7.3"
  }
}
